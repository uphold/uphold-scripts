{
  "clearMocks": true,
  "collectCoverage": true,
  "collectCoverageFrom": [
    "*/**/*.js",
    "!config/**/*",
    "!coverage/**/*",
    "!test/**/*"
  ],
  "coverageReporters": [
    "json",
    "html",
    "text"
  ],
  "modulePaths": [
    "<rootDir>"
  ],
  "restoreMocks": true,
  "testEnvironment": "node",
  "testRegex": "((\\.)(test|spec))\\.js$"
}
